        "testimonial_author_image": {
          "url": "https://8310787.fs1.hubspotusercontent-na1.net/hubfs/8310787/pexels-sora-shimazaki-5668773_crop_case_study.jpg",
          "width": 200,
          "height": 200,
          "type": "image",
          "alt": "Dr. Michael Chen headshot"
